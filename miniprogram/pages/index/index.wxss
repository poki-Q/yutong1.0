.layout{
  width: 100%;
  background-color: rgb(255, 255, 255)
}

/* banner */
.banner{
  width: 100%;
  height: 150px;
  background-color:rgba(255, 255, 255, 1);
  color: black;
  text-align: center;
  padding-top: 30px;  
  box-sizing: border-box;
  font-size: 15px;
}

.banner view{
  margin-top: 10px;
}

.title{
  margin: 0 auto;
  border-radius: 50%;
}
.title image{
  width: 80px;
  height: 80px;
  border-radius: 50%;
}
/* items */
.items{
  overflow: hidden;
  background-color:rgb(240,240,240);
  padding-bottom: 50px;
}

.layout .item{
  width: 90%;
  background-color:white;
  color: black;
  height: 70px;
  line-height: 70px;
  text-align: center;
  font-size: 20px;
  margin: 0 auto;
  margin-bottom: 20px;
  margin-top: 10px;
  border-radius: 5px;
  box-shadow: 0 18px 5px -8px #333;
  box-sizing: border-box;
}